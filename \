package Business::CyberSource::Request;
use 5.008;
use strict;
use warnings;

# VERSION

use Moose;
use namespace::autoclean;

with qw(
	Business::CyberSource::Request::Role::Credentials
);

use MooseX::AbstractFactory;

around 'create' => sub {
	my $orig = shift;
	my $self = shift;
	my ( $imp, $args ) = @_;
	
	$self->orig( $imp );
};

1;

# ABSTRACT: CyberSource request factory
